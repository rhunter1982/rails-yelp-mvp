<div class="container">
  <%= simple_form_for(@restaurant) do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :category, collection: Restaurant::CATEGORY %>
    <%= f.submit class:"btn btn-primary" %>
  <% end %>
    <br>
  <%= link_to "back to Resturants", restaurants_path, class:"btn btn-primary"  %>
</div>
